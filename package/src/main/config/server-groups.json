{
  "databases": {
    "*": {
      "groups": {
        "admin": {
          "access": ["updateSecurity", "updateSchema"],
          "resultSetLimit": -1,
          "readTimeout": -1,
          "types": {
            "*": {
              "access": ["createRecord", "readRecord", "updateRecord", "deleteRecord"]
            }
          }
        },
        "readonly": {
          "access": [],
          "resultSetLimit": -1,
          "readTimeout": -1,
          "types": {
            "*": {
              "access": ["readRecord"]
            }
          }
        },
        "*": {
          "access": [],
          "resultSetLimit": -1,
          "readTimeout": -1,
          "types": {
            "*": {
              "access": []
            }
          }
        }
      }
    }
  },
  "version": 1
}
