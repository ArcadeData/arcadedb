{
  "name": "arcadedb-studio",
  "version": "25.8.1",
  "description": "ArcadeDB Studio Frontend - Multi-Model Database Administration Interface",
  "main": "src/main/resources/static/index.html",
  "scripts": {
    "audit": "npm audit",
    "audit-fix": "npm audit fix",
    "update": "npm update",
    "outdated": "npm outdated",
    "security-check": "npm audit --audit-level=moderate",
    "security-audit": "./scripts/security-audit.sh",
    "copy-swagger-ui": "node scripts/copy-swagger-ui.js",
    "prebuild": "npm run copy-swagger-ui",
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "clean": "rm -rf src/main/resources/static/dist node_modules",
    "precommit": "npm run security-check && npm run build",
    "prepare": "npm run security-check",
    "version": "npm run security-check && npm run build && git add src/main/resources/static/dist package.json package-lock.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.1",
    "apexcharts": "^3.45.2",
    "bootstrap": "^5.3.8",
    "clipboard": "^2.0.11",
    "codemirror": "^5.65.18",
    "cytoscape": "^3.33.1",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-cxtmenu": "^3.5.0",
    "cytoscape-graphml": "^1.0.6",
    "cytoscape-node-html-label": "^1.2.2",
    "datatables.net": "^2.3.4",
    "datatables.net-bs5": "^2.3.4",
    "datatables.net-buttons": "^3.2.5",
    "datatables.net-buttons-bs5": "^3.2.5",
    "datatables.net-responsive": "^3.0.7",
    "datatables.net-responsive-bs5": "^3.0.7",
    "datatables.net-select": "^3.1.2",
    "datatables.net-select-bs5": "^3.1.2",
    "jquery": "^3.7.1",
    "jszip": "^3.10.1",
    "notyf": "^3.10.0",
    "pdfmake": "^0.2.13",
    "swagger-ui-dist": "^5.29.3",
    "sweetalert2": "^11.23.0",
    "webcola": "^3.4.0"
